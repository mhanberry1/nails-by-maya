@import url("common.css");

nav {
    .custom {
        text-decoration: underline;
    }
}

main {
    text-align: center;
    padding-top: 25px;
    height: 500px;

    h1 {
        font-family: 'Corinthia', cursive;
        font-size: 60px;
        font-weight: normal;
        color: var(--blue);
    }

    p {
        text-align: left;
        margin: 50px;
        width: 400px;
        display: inline-block;
    }

    img {
        width: 300px;
        display: inline-block;

    }

}
